UNITS LENGTH=A TIME=fs ENERGY=kcal/mol
#----Read colvars in-------
proj: READ FILE=cmap_highE/COLVAR_cat__FORCE_.dat  VALUES=proj IGNORE_FORCES IGNORE_TIME
ext: READ FILE=cmap_highE/COLVAR_cat__FORCE_.dat  VALUES=ext IGNORE_FORCES IGNORE_TIME
#cnum: READ FILE=cmap_highE/COLVAR_cat__FORCE_.dat  VALUES=cnum IGNORE_FORCES IGNORE_TIME
#d1: READ FILE=cmap_highE/COLVAR_cat__FORCE_.dat  VALUES=cmap IGNORE_FORCES IGNORE_TIME
cmap: READ FILE=cmap_highE/CMAP_cat__FORCE_.dat  VALUES=cmap IGNORE_FORCES IGNORE_TIME
#----Read biases ------- 
#wall: READ FILE=cmap_highE/WALL_cat__FORCE_.dat VALUES=totalwall.bias IGNORE_FORCES IGNORE_TIME
#pull: READ FILE=cmap_highE/PBIAS_cat__FORCE_.dat VALUES=p.bias IGNORE_FORCES IGNORE_TIME
wham: READ FILE=cmap_highE/BIAS_cat__FORCE_.dat VALUES=wham.bias IGNORE_FORCES IGNORE_TIME

#----get weights-------
weights: REWEIGHT_BIAS ARG=wham.bias TEMP=300.0

#---Histogram with weights----
#----1D-----
projhist: HISTOGRAM ARG=proj STRIDE=1 GRID_MIN=_MINP_ GRID_MAX=_MAXP_ GRID_BIN=200 BANDWIDTH=0.06 LOGWEIGHTS=weights 
exthist: HISTOGRAM ARG=ext STRIDE=1 GRID_MIN=_MINE_ GRID_MAX=_MAXE_ GRID_BIN=200 BANDWIDTH=0.06 LOGWEIGHTS=weights 
#cnumhist: HISTOGRAM ARG=cnum STRIDE=1 GRID_MIN=1 GRID_MAX=15 GRID_BIN=100 BANDWIDTH=0.08 LOGWEIGHTS=weights 
cmaphist: HISTOGRAM ARG=cmap STRIDE=1 GRID_MIN=0.0 GRID_MAX=0.9 GRID_BIN=100 BANDWIDTH=0.02 LOGWEIGHTS=weights 

#----2D----
proj_ext: HISTOGRAM ARG=proj,ext STRIDE=1  GRID_MIN=_MINP_,_MINE_ GRID_MAX=_MAXP_,_MAXE_ GRID_BIN=100,100 BANDWIDTH=0.07,0.07 LOGWEIGHTS=weights 
#proj_cnum: HISTOGRAM ARG=proj,cnum STRIDE=1 GRID_MIN=_MINP_,1 GRID_MAX=_MAXP_,15 GRID_BIN=100,100 BANDWIDTH=0.1,0.15 LOGWEIGHTS=weights 
#ext_cnum: HISTOGRAM ARG=ext,cnum STRIDE=1 GRID_MIN=_MINE_,1 GRID_MAX=_MAXE_,15 GRID_BIN=100,100 BANDWIDTH=0.1,0.15 LOGWEIGHTS=weights
proj_cmap: HISTOGRAM ARG=proj,cmap STRIDE=1 GRID_MIN=_MINP_,0 GRID_MAX=_MAXP_,0.9 GRID_BIN=100,100 BANDWIDTH=0.1,0.08 LOGWEIGHTS=weights
ext_cmap: HISTOGRAM ARG=ext,cmap STRIDE=1 GRID_MIN=_MINE_,0 GRID_MAX=_MAXE_,0.9 GRID_BIN=100,100 BANDWIDTH=0.1,0.08 LOGWEIGHTS=weights

#---Convert to FES----
feproj: CONVERT_TO_FES GRID=projhist TEMP=300 MINTOZERO
feext: CONVERT_TO_FES GRID=exthist TEMP=300 MINTOZERO
#fecnum: CONVERT_TO_FES GRID=cnumhist TEMP=300 MINTOZERO
fecmap: CONVERT_TO_FES GRID=cmaphist TEMP=300 MINTOZERO


fftwocv: CONVERT_TO_FES GRID=proj_ext TEMP=300 MINTOZERO
#ffprojcnum: CONVERT_TO_FES GRID=proj_cnum TEMP=300 MINTOZERO
#ffextcnum: CONVERT_TO_FES GRID=ext_cnum TEMP=300 MINTOZERO
ffprojcmap: CONVERT_TO_FES GRID=proj_cmap TEMP=300 MINTOZERO
ffextcmap: CONVERT_TO_FES GRID=ext_cmap TEMP=300 MINTOZERO

#DUMPGRID GRID=fecnum  FILE=cmap_highE/F_FORCE__fes_cnum_rw.dat FMT=%.24e
DUMPGRID GRID=feproj  FILE=cmap_highE/F_FORCE__fes_proj_rw.dat FMT=%.24e
DUMPGRID GRID=feext  FILE=cmap_highE/F_FORCE__fes_ext_rw.dat FMT=%.24e
DUMPGRID GRID=fecmap  FILE=cmap_highE/F_FORCE__fes_cmap_rw.dat FMT=%.24e

DUMPGRID GRID=fftwocv FILE=cmap_highE/F_FORCE__fes_projext_rw.dat FMT=%.24e
#DUMPGRID GRID=ffprojcnum FILE=cmap_highE/F_FORCE__fes_projcnum_rw.dat FMT=%.24e
#DUMPGRID GRID=ffextcnum FILE=cmap_highE/F_FORCE__fes_extcnum_rw.dat FMT=%.24e
DUMPGRID GRID=ffprojcmap  FILE=cmap_highE/F_FORCE__fes_projcmap_rw.dat FMT=%.24e
DUMPGRID GRID=ffextcmap  FILE=cmap_highE/F_FORCE__fes_extcmap_rw.dat FMT=%.24e

PRINT ARG=proj,ext,cmap,wham.bias,weights STRIDE=1 FILE=cmap_highE/COLVAR_cat__FORCE__weights.out
